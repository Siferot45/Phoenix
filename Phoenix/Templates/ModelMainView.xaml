<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:system="clr-namespace:System;assembly=System.Runtime"
                    xmlns:evm="clr-namespace:Phoenix.ViewModels.EntityViewModel"
                    xmlns:vm="clr-namespace:Phoenix.ViewModels"
                    xmlns:view="clr-namespace:Phoenix.Views" 
                    xmlns:viewWindow="clr-namespace:Phoenix.Views.Windows">

    <DataTemplate DataType="{x:Type evm:MassageViewModel}">
        <view:MassageView DataContext="{Binding}"/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type evm:ClientsViewModel}">
        <view:ClientsView DataContext="{Binding}"/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:CategoryEditorViewModel}">
        <view:ClientsView DataContext="{Binding}"/>
    </DataTemplate>

</ResourceDictionary>