<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:system="clr-namespace:System;assembly=System.Runtime"
                    xmlns:vm="clr-namespace:Phoenix.ViewModels.EntityViewModel"
                    xmlns:view="clr-namespace:Phoenix.Views">
    
    <DataTemplate DataType="{x:Type vm:MassageViewModel}">
        <view:MassageView DataContext="{Binding}"/>
    </DataTemplate>
    
    <DataTemplate DataType="{x:Type vm:ClientsViewModel}">
        <view:ClientsView DataContext="{Binding}"/>
    </DataTemplate>
</ResourceDictionary>